Resources:
  httpListener:
    Type: AWS::ElasticLoadBalancingV2::Listener
    Properties:
      DefaultActions:
        - Type: "redirect"
          RedirectConfig:
            Protocol: HTTPS
            Port: "443"
            StatusCode: HTTP_301
      LoadBalancerArn: "arn:aws:elasticloadbalancing:us-west-2:381492034918:loadbalancer/app/your-load-balancer-arn"
      Port: 80
      Protocol: HTTP
